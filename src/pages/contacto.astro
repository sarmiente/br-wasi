---
import Layout from "../layouts/Layout.astro";
import Form from "../components/sections/Form.astro";


---


<Layout title="Contacto">
     
     
<section class="bg-white dark:bg-gray-900 pt-36">
    <!-- Header de la sección -->
    <div class="relative h-auto bg-gray-100 dark:bg-gray-800 py-10">
      <div class="container px-6 mx-auto">
        <h1 class="text-sm font-pattern uppercase text-gray-600 text-center">Déjanos un mensaje</h1>
        <h2 class="mt-2 text-center text-2xl font-heading uppercase text-gray-900 sm:text-4xl dark:text-white">
          Hagamos algo nuevo juntos
        </h2>
      </div>
    </div>
  
  </section>
  
<section class="bg-gray-100 ">
    <div class="mx-auto max-w-4xl px-4 py-10 sm:px-6 lg:px-8">

      
        <Form />

    </div>
  </section>


</Layout>